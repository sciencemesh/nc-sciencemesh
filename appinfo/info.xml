    <dependencies>
        <nextcloud min-version="28" max-version="28"/>
        <owncloud min-version="10" max-version="10"/>
    </dependencies>

    <settings>
        <admin>OCA\ScienceMesh\Settings\SciencemeshSettingsAdmin</admin>
    </settings>
    <settings-sections>
        <admin>OCA\ScienceMesh\Sections\SciencemeshSettingsAdmin</admin>
    </settings-sections>

    <use-migrations>true</use-migrations>

    <navigations>
        <navigation>
            <name>ScienceMesh</name>
            <route>sciencemesh.app.contacts</route>
            <order>10</order>
        </navigation>
    </navigations>
